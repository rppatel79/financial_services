{
    "containerDefinitions": [
        {
            "name": "analytics",
            "image": "541752449603.dkr.ecr.us-east-1.amazonaws.com/analytics",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "analytics-95-tcp",
                    "containerPort": 95,
                    "hostPort": 95,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "securitymaster.uri",
                    "value": "http://security_master.financial_services_cluster:80"
                },
                {
                    "name": "marketdata.uri",
                    "value": "http://market_data.financial_services_cluster:90"
                }
            ],
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/analytics_service",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "family": "analytics_service",
    "taskRoleArn": "arn:aws:iam::541752449603:role/financialservices-ecsTaskRole",
    "executionRoleArn": "arn:aws:iam::541752449603:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [],
    "placementConstraints": [],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "512",
    "memory": "1024",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    },
    "tags": [
        {
            "key": "app",
            "value": "financialservices"
        },
        {
            "key": "ecs:taskDefinition:createdFrom",
            "value": "ecs-console-v2"
        }
    ]
}
